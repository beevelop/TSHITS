#!/usr/bin/env bash

export SERVICE="Nexus"
export WAIT_TIME=60

do_health() {
  STATUS=0
  check_traefik ${SERVICE_DOMAIN} "HTTP/1.1 200 OK" || STATUS=$?
  return $STATUS
}

